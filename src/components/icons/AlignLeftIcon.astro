---
import type { Pixel } from '../../model/model';

export interface Props {
  readonly height: Pixel;
  readonly width: Pixel;
}

const { height, width } = Astro.props;
---

<svg
  fill='none'
  height={height}
  width={width}
  stroke='currentColor'
  stroke-linecap='round'
  stroke-linejoin='round'
  stroke-width='2'
  viewBox='0 0 24 24'>
    <line x1='17' x2='3' y1='10' y2='10'></line>
    <line x1='21' x2='3' y1='6' y2='6'></line>
    <line x1='21' x2='3' y1='14' y2='14'></line>
    <line x1='17' x2='3' y1='18' y2='18'></line>
  </svg>
